   "source": [
    "from igraph import Graph, plot\n",
    "\n",
    "g = Graph().as_directed()\n",
    "\n",
    "g.add_vertex('Kara')\n",
    "g.add_vertex('David')\n",
    "g.add_vertex('Mel')\n",
    "\n",
    "g.vs[\"label\"] = g.vs[\"name\"]\n",
    "\n",
    "g.add_edges([('Kara', 'David'), ('David', 'Mel'), ('Mel', 'Kara')])\n",
    "\n",
    "plot(g, vertex_label_size=20, vertex_label_dist=3)"
   ]
